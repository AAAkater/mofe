{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Mofe Server",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "fastapi",
      "args": ["dev", "./app/main.py"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "consoleName": "Mofe Server"
    }
  ]
}
